{
  "name": "vite-react-darklight",
  "private": true,
  "description": "Vite React Dark/Light Template",
  "author": {
    "name": "andron13",
    "email": "andron13@gmail.com",
    "url": "https://github.com/andron13"
  },
  "repository": {
    "type": "git",
