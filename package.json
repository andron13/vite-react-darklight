    "format:fix": "prettier --write \"./**/*.{ts,tsx,css,mjs,json,md}\"",
    "fix:all": "npm run typecheck && npm run format:fix && npm run lint:fix",
    "ncu:u": "ncu -u",
    "prepare": "husky"
